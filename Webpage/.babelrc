{
    "presets": [
        [
            "@babel/preset-env", {
                "modules": false
            }
        ],
        "@babel/react",
        [
            "@babel/typescript", { 
                "isTSX": true, 
                "allExtensions": true 
            }
        ]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        [
            "@babel/syntax-typescript", { 
                "isTSX": true
            }
        ]
    ]
}